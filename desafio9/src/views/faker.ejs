<%- include("layout/main", {title: "Faker"}) %>
<%- include("partials/nav.ejs") %>

<body>
  <main>
    <div class="section-products">
      <% if(products) { %>
      <table width="60%">
        <thead>
          <tr>
            <td>Title</td>
            <td>Price</td>
            <td>Thumbnail</td>
          </tr>
        </thead>
        <tbody>
          <% products.forEach((item)=>{ %>
          <tr key="<%= item.id %>">
            <td><%= item.title %></td>
            <td>$AR <%= item.price %></td>
            <td>
              <img
                alt="<%= item.title %>"
                src="<%= item.thumbnail %>"
                width="40px"
                height="40px"
              />
              <p><%= item.thumbnail %></p>
            </td>
          </tr>
          <% }) %>
        </tbody>
      </table>
      <% } else { %>
      <h1>This is empty --- DEBIERON BANEAR AATROX</h1>
      <% } %>
    </div>
  </main>
</body>
</html>